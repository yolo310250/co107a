/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in = b , out = notbn);
    And(a = notb , b = sel ,out =selb);
    And(a = a ,b = b,out = ab);
    Or(a = selb, b = ab,out = out);
}
